<%= form_for @list do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, placeholder: "Title" %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, placeholder: "Notes" %>
  </div>
  <%= f.submit "Submit", class: "btn btn-sm btn-success" %>
<% end %>
